{
    "name": "tapp/filament-confetti",
    "description": "A Filament plugin for beautiful confetti animations using canvas-confetti",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tapp Network"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^11.0|^12.0",
        "filament/filament": "^4.0|^5.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.0",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Tapp\\FilamentConfetti\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tapp\\FilamentConfetti\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tapp\\FilamentConfetti\\FilamentConfettiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
